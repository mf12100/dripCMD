SELECT
  'Drip Hydration' AS location,
  id,
  object,
  created,
  invoice,
  currency,
  CAST(amount AS FLOAT64) / 100 AS total_revenue,
  metadata.email,
  metadata.source,
  metadata.treatment,
  metadata.promo,
  metadata.provider,
  metadata.service_category, -- Added service_category
  source AS source_field,
  customer,
  last_payment_error.code,
  last_payment_error.type
FROM `trusty-hangar-396923.drip_hydration.payment_intents`

UNION ALL

